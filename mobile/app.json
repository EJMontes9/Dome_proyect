{
  "expo": {
    "name": "Moodle Mobile",
    "slug": "moodle-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#1e88e5"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.moodlemobile.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1e88e5"
      },
      "package": "com.moodlemobile.app"
    },
    "plugins": ["expo-secure-store"]
  }
}
